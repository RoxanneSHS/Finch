TakePartyItem("SUFBOOK3")
DestroyItem("SUFBOOK3")
TakePartyItem("SUFBOOK4")
DestroyItem("SUFBOOK4")
TakePartyItem("SUFBOOKA")
DestroyItem("SUFBOOKA")
TakePartyItem("SUFBOOKB")
