PartyHasItem("SUFBOOK2")
PartyHasItem("SUFBOOK3")
PartyHasItem("SUFBOOK4")
PartyHasItem("SUFBOOKA")
PartyHasItem("SUFBOOKB")
THEN
	RESPONSE #100
SetGlobal("SUFinchLibraryItem", "GLOBAL", 1)
TakePartyItem("SUFBOOK1")
DestroyItem("SUFBOOK1")
